{
  "development": {
		"use_env_variable": "DATABASE_URL",
		"dialect": "postgres",
		"dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      },
			"useUTC": true
		},
		"pool": {
			"max": 5,
			"min": 0,
			"acquire": 30000,
			"idle": 10000
		},
		"timezone" : "+00:00",
		"minifyAliases": true,
		"logging": true,
		"timestamps": false
  },
  "production": {
		"use_env_variable": "DATABASE_URL",
		"dialect": "postgres",
		"dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      },
			"useUTC": true
		},
		"pool": {
			"max": 5,
			"min": 0,
			"acquire": 30000,
			"idle": 10000
		},
		"timezone" : "+00:00",
		"minifyAliases": true,
		"logging": false
  }
}
